{
  "name": "web-application",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44426 --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
    "start:default": "ng serve --port 44426 --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "build": "ng build",
    "build:ssr": "ng run ASP.NETCoreWebApplication1:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "web-types": "./spectrum-web-types.json",
  "dependencies": {
    "@adobe/spectrum-css-grid": "^0.5.0",
    "@angular/animations": "~13.0.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/platform-server": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@ngneat/until-destroy": "^9.2.1",
    "@ngrx/component": "^13.2.0",
    "@spectrum-css/dialog": "^6.0.15",
    "@spectrum-css/table": "^4.0.19",
    "@spectrum-css/typography": "^4.0.20",
    "@spectrum-web-components/bundle": "^0.26.0",
    "include-media": "^1.4.10",
    "oidc-client": "^1.11.5",
    "run-script-os": "^1.1.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "xregexp": "^5.1.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular-devkit/build-angular": "~13.0.1",
    "@angular/cli": "~13.0.1",
    "@angular/compiler-cli": "~13.0.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prismjs": "^1.28.0",
    "raw-loader": "^4.0.2",
    "typescript": "~4.4.3"
  }
}
